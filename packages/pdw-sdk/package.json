{
  "name": "@personal-data-wallet/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Personal Data Wallet - Decentralized memory system with SEAL encryption and Walrus storage",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run codegen && npm run build:ts",
    "build:ts": "tsc && tsc -p tsconfig.esm.json",
    "codegen": "sui-ts-codegen generate",
    "codegen:summaries": "cd ../../smart-contract && sui move summary",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sui",
    "blockchain",
    "memory",
    "encryption",
    "seal",
    "walrus",
    "decentralized",
    "sdk",
    "typescript"
  ],
  "author": "Personal Data Wallet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CommandOSSLabs/personal-data-wallet.git",
    "directory": "packages/pdw-sdk"
  },
  "peerDependencies": {
    "@mysten/sui": "^1.37.0"
  },
  "dependencies": {
    "@mysten/codegen": "^0.5.0",
    "@mysten/seal": "^0.6.0",
    "@mysten/walrus": "^0.5.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}