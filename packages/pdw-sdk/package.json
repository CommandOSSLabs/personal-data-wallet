{
  "name": "personal-data-wallet-sdk",
  "version": "0.2.2",
  "description": "TypeScript SDK for Personal Data Wallet - Decentralized memory system with AI embeddings, HNSW vector search, SEAL encryption and Walrus storage",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run codegen && npm run build:ts",
    "build:ts": "tsc",
    "codegen": "sui-ts-codegen generate",
    "codegen:summaries": "cd ../../smart-contract && sui move summary",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:seal": "jest --testPathPattern=seal-deployment --verbose",
    "test:seal:watch": "jest --testPathPattern=seal-deployment --watch --verbose",
    "test:deploy": "npm run build && npm run test:seal",
    "verify:deployment": "node scripts/verify-deployment.js",
    "verify:quick": "npx jest test/simple-config.test.ts && npm run verify:deployment",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "_prepublishOnly": "npm run build"
  },
  "keywords": [
    "sui",
    "blockchain",
    "memory",
    "ai",
    "embeddings",
    "vector-search",
    "hnsw",
    "encryption",
    "seal",
    "walrus",
    "decentralized",
    "sdk",
    "typescript"
  ],
  "author": "Personal Data Wallet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CommandOSSLabs/personal-data-wallet.git",
    "directory": "packages/pdw-sdk"
  },
  "peerDependencies": {
    "@mysten/dapp-kit": "^0.19.0",
    "@mysten/sui": "^1.38.0",
    "@tanstack/react-query": "^5.90.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.20.0",
    "@mysten/bcs": "^1.8.0",
    "@mysten/codegen": "^0.5.0",
    "@mysten/seal": "^0.6.0",
    "@mysten/slush-wallet": "^0.2.1",
    "@mysten/utils": "^0.2.0",
    "@mysten/walrus": "^0.7.0",
    "hnswlib-wasm": "^0.8.2",
    "sha3": "^2.1.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
