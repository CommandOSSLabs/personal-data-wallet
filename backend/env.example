# Server configuration
PORT=3000
NODE_ENV=development

# Sui blockchain configuration
SUI_NETWORK=devnet
SUI_PACKAGE_ID=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
SUI_ADMIN_PRIVATE_KEY=your_private_key_here

# Google Gemini API
GOOGLE_API_KEY=your_gemini_api_key_here

# Walrus storage configuration
WALRUS_API_URL=https://api.walrus.ai/v1
WALRUS_API_KEY=your_walrus_api_key_here

# Seal encryption (Identity-Based Encryption)
# Network to use for SEAL (mainnet, testnet, devnet)
SEAL_NETWORK=testnet
# Package ID of your deployed SEAL Move contract
SEAL_PACKAGE_ID=0x62c79dfeb0a2ca8c308a56bde530ccf3846535e1623949d45c90d23128afff52
# Module name containing the seal_approve function
SEAL_MODULE_NAME=access_control
# Threshold number of key servers required for decryption
SEAL_THRESHOLD=2
# Session key TTL in minutes
SEAL_SESSION_TTL_MIN=60
# Optional: Specific key server IDs (comma-separated), if empty will use allowlisted servers
SEAL_KEY_SERVER_IDS=
# Optional: Custom Sui RPC URL, if empty will use default for network
SUI_RPC_URL=
# Enable SEAL open mode (true = open mode, false = permissioned mode)
SEAL_OPEN_MODE=true